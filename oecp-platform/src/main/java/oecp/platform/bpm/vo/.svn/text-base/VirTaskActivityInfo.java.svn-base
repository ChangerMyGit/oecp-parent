package oecp.platform.bpm.vo;

/**
 *
 * @author yangtao
 * @date 2011-7-29下午03:52:56
 * @version 1.0
 */
public class VirTaskActivityInfo {
	//虚拟节点ID
	private String virProActivityId;
	//流程定义部署ID
	private String deployId;
	//流程名称
	private String processName;
	//节点名称
	private String activityName;
	//虚拟流程ID
	private String virProDefinitionId;
	//流程所在的组织
	private String assignOrgId;

	/**
	 * @return the deployId
	 */
	public String getDeployId() {
		return deployId;
	}
	/**
	 * @param deployId the deployId to set
	 */
	public void setDeployId(String deployId) {
		this.deployId = deployId;
	}
	/**
	 * @return the processName
	 */
	public String getProcessName() {
		return processName;
	}
	/**
	 * @param processName the processName to set
	 */
	public void setProcessName(String processName) {
		this.processName = processName;
	}
	/**
	 * @return the virProActivityId
	 */
	public String getVirProActivityId() {
		return virProActivityId;
	}
	/**
	 * @param virProActivityId the virProActivityId to set
	 */
	public void setVirProActivityId(String virProActivityId) {
		this.virProActivityId = virProActivityId;
	}
	/**
	 * @return the activityName
	 */
	public String getActivityName() {
		return activityName;
	}
	/**
	 * @param activityName the activityName to set
	 */
	public void setActivityName(String activityName) {
		this.activityName = activityName;
	}
	/**
	 * @return the virProDefinitionId
	 */
	public String getVirProDefinitionId() {
		return virProDefinitionId;
	}
	/**
	 * @param virProDefinitionId the virProDefinitionId to set
	 */
	public void setVirProDefinitionId(String virProDefinitionId) {
		this.virProDefinitionId = virProDefinitionId;
	}
	/**
	 * @return the assignOrgId
	 */
	public String getAssignOrgId() {
		return assignOrgId;
	}
	/**
	 * @param assignOrgId the assignOrgId to set
	 */
	public void setAssignOrgId(String assignOrgId) {
		this.assignOrgId = assignOrgId;
	}
	
	
	
}
